<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/img/icons/ms-icon-70x70.png"/>
      <square150x150logo src="/img/icons/ms-icon-150x150.png"/>
      <square310x310logo src="/img/icons/ms-icon-310x310.png"/>
      <wide310x150logo src="/img/icons/ms-icon-310x150.png"/>
      <TileColor>#004070</TileColor>
    </tile>
    <notification>
      <polling-uri src="https://dratatianaleal.com/feed-es.xml"/>
      <polling-uri2 src="https://dratatianaleal.com/feed-en.xml"/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
    <badge>
      <polling-uri src="/notification-badge.xml"/>
    </badge>
  </msapplication>
</browserconfig>